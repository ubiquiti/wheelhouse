{
  "name": "wheelhouse",
  "version": "0.2.0",
  "description": "An application template built on Flatiron and Backbone.",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "start": "node index.js",
    "server-start": "supervisor -w app -e 'js|json|hbs' index.js",
