    "grunt": "~0.4",
    "grunt-bumpx": "~0.1",
    "grunt-notify": "~0.2",
    "grunt-contrib-jshint": "~0.5",
    "grunt-contrib-watch": "~0.4",
    "grunt-contrib-connect": "~0.2",
    "grunt-contrib-handlebars": "~0.5",
    "grunt-mocha": "~0.3",
    "sinon-chai": "~2.3",
    "chai": "~1.5",
    "mocha": "~1.9",
    "chai-jquery": "~1.1",
    "grunt-shell": "~0.2",
    "grunt-contrib-imagemin": "~0.1",
    "grunt-contrib-clean": "~0.4",
